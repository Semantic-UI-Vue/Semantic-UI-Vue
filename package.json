  "license": "MIT",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build:docs": "node build/build-docs.js",
    "build:commonjs": "babel src -d dist/commonjs",
    "build:umd": "node build/build-umd.js",
    "build": "npm run build:docs && npm run build:commonjs && npm run build:umd",
    "test": "npm run unit",
    "test:watch": "npm run unit:watch",
    "test:update": "cross-env BABEL_ENV=test UPDATE=1 PRUNE=1 karma start test/karma.conf.js --single-run",
    "unit": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "unit:watch": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "lint": "eslint --ext .js,.vue,.jsx docs src test/specs",
    "report-coverage": "codecov"
  },
